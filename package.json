{
	"name": "fresnel",
	"version": "1.0.0",
	"license": "MIT",
	"description": "Create and compare web performance reports.",
	"keywords": [
		"performance",
		"metrics",
		"perfmatters"
	],
	"homepage": "https://github.com/wikimedia/fresnel",
	"repository": {
		"type": "git",
		"url": "https://github.com/wikimedia/fresnel"
	},
	"bin": "bin/fresnel.js",
	"files": [
		"bin/",
		"src/",
		"index.js",
		"npm-shrinkwrap.json"
	],
	"engines": {
		"node": ">=10.0.0"
	},
	"dependencies": {
		"js-yaml": "3.13.1",
		"puppeteer": "4.0.1"
	},
	"devDependencies": {
		"eslint-config-wikimedia": "0.20.0",
		"jsdoc-to-markdown": "6.0.1",
		"mock-require": "3.0.3",
		"nyc": "15.1.0",
		"qunit": "2.15.0",
		"rimraf": "3.0.2"
	},
	"scripts": {
		"test": "nyc qunit && eslint . && npm run doc",
		"qunit": "qunit",
		"doc": "jsdoc2md --template build/api.partial.hbs src/*.js src/**/*.js > API.md",
		"doc:dump": "jsdoc2md --namepaths src/*.js src/**/*.js",
		"changelog": "git log --format='* %s (%aN)' --no-merges $(node -e 'console.log(require(\"./package.json\").version);')...HEAD | sort | grep -vE '^\\* (build|docs?|tests?):'"
	},
	"nyc": {
		"exclude": [
			"test"
		],
		"reporter": [
			"text",
			"html",
			"clover"
		],
		"report-dir": "coverage",
		"check-coverage": true,
		"functions": 95,
		"lines": 95,
		"skip-full": true,
		"watermarks": {
			"statements": [
				0,
				99
			],
			"branches": [
				0,
				99
			],
			"functions": [
				95,
				99
			],
			"lines": [
				95,
				99
			]
		}
	}
}
